@using Backend.Models.Features.Jobs
@using Frontend.Features.Jobs.Components
@using Frontend.Features.Jobs.Models

@if (Jobs != null)
{
    foreach (var model in Jobs)
    {
        <JobComponent Job="@model" OnJobUpdated="@OnJobUpdated"/>
    }
}

@code
{
    [Parameter]
    public List<JobModelView>? Jobs { get; set; }

    [Parameter]
    public Func<JobModelView, Task>? OnJobUpdated { get; set; }
}